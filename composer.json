{
  "name": "impresscms/composer-addon-installer-plugin",
  "description": "This plugin allows the installation of ImpressCMS addons from composer",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "David Janssens",
      "email": "david.j@impresscms.org"
    }
  ],
  "type": "composer-plugin",
  "autoload": {
    "psr-4": {
      "ImpressCMS\\Composer\\AddonInstaller\\": "src/"
    }
  },
  "extra": {
    "class": "ImpressCMS\\Composer\\AddonInstaller\\ComposerPlugin"
  },
  "require": {
    "composer-plugin-api": "^1.0 || ^2.0",
    "vlucas/phpdotenv": "^5.5.0",
    "php": ">7.2.5",
    "imponeer/env": "^1.0.0"
  },
  "require-dev": {
    "composer/composer": "^1.10.23 || ^2.1.9"
  }
}
