{
    "name": "impresscms/composer-module-installer-plugin",
    "description": "This plugin allows the installation of ImpressCMS addons within composer",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "David Janssens",
            "email": "david.j@impresscms.org"
        }
    ],
    "type": "composer-plugin",
    "autoload": {
        "psr-4": {"ImpressCMS\\Composer\\": "src/"}
    },
    "extra": {
        "class": "ImpressCMS\\Composer\\ModuleInstallerPlugin",
        "class": "ImpressCMS\\Composer\\ThemeInstallerPlugin",
        "class": "ImpressCMS\\Composer\\PreloadInstallerPlugin",
        "class": "ImpressCMS\\Composer\\LanguageInstallerPlugin"
    },
    "require": {
        "composer-plugin-api": "1.0.0"
    }
}
