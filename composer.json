{
    "name": "impresscms/composer-module-installer-plugin",
    "description": "This plugin allows the installation of ImpressCMS addons from composer",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "David Janssens",
            "email": "david.j@impresscms.org"
        }
    ],
    "type": "composer-plugin",
    "autoload": {
        "psr-4": {
            "ImpressCMS\\Composer\\AddonInstaller\\": "src/"
        }
    },
    "extra": {
        "class": "ImpressCMS\\Composer\\AddonInstaller\\ComposerPlugin"
        },
    "require": {
        "symfony/polyfill-php80": "^1.21",
        "composer-plugin-api": "^1.0 || ^2.0"
    },
  "require-dev": {
    "composer/composer": "^1.0 || ^2.0.14"
  }
}
